r\nimport { Button } from 'antd';\r\nimport { PlusOutlined } from '@ant-design/icons';\r\n\r\nconst NavBar = ({ user }) => {\r\n  const location = useLocation();\r\n  const history = useHistory();\r\n  const params = useParams();\r\n  const subTitle = user === null ? '' : `Welcome ${user.name}`;\r\n  const [currentPage, setCurrentPage] = useState(null);\r\n\r\n  // Following hook checks what is the location and therefore\r\n  // Based on it displays neccessary extras, i.e. buttons\r\n  useEffect(() => {\r\n    // TODO: Apparently there is a need for a hack\r\n    console.log(location.pathname, params, location);\r\n    setCurrentPage(location.pathname);\r\n  }, [location.pathname]);\r\n\r\n  const returnAddSpending = () => {\r\n    return currentPage === '/personal-plan' ? (\r\n      <Button key=\"2\" onClick={() => history.push('/spending-form')}>\r\n        <PlusOutlined />\r\n        Add expense\r\n      </Button>\r\n    ) : null;\r\n  };\r\n\r\n  return (\r\n    <div style={{ border: '1px solid black' }}>\r\n      <PageHeader\r\n        title=\"Budget App\"\r\n        subTitle={subTitle}\r\n        extra={[returnAddSpending(), user ? <DropdownMenu key=\"1\" /> : null]}\r\n      />\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default NavBar;\r\n","/app/src/components/pages/OverviewPerMonth.js",[],"/app/src/components/pages/OverviewDropdown.js",[],"/app/src/components/pages/OverviewFilterNavbar.js",[],"/app/src/components/pages/OverviewPerYear.js",[],"/app/src/services/userService.js",[],"/app/src/functions/helperFunctions.js",[],"/app/src/components/shared/RemoveButton.js",[],"/app/src/components/shared/ModifyButton.js",[],"/app/src/components/shared/ChangePassword.js",[],"/app/src/components/shared/DropdownMenu.js",[],"/app/src/components/forms/CustomSelectCategory.js",[],"/app/src/functions/overviewFormatters.js",[],"/app/src/functions/overviewDropdown.js",[],"/app/src/components/shared/Logout.js",[],"/app/src/functions/dateFromatters.js",[],"/app/src/reducers/familyPlanReducer.js",[],"/app/src/services/familyPlanService.js",[],"/app/src/components/pages/PersonalExpensesContainer.js",[],"/app/src/components/pages/FamilyExpensesContainer.js",[],"/app/src/components/pages/FamilyPlanHome/FamilyPlanHome.js",[],"/app/src/components/pages/FamilyPlanHome/extra/FamilyPlansList.js",[],"/app/src/components/pages/FamilyPlanHome/extra/FamilyPlanCard.js",[],"/app/src/components/pages/FamilyPlanHome/extra/InvitePeople.js",["181"],"import React, { useState, useEffect, useMemo } from 'react';\r\nimport { Input, Select, message } from 'antd';\r\nimport { useSelector, useDispatch } from 'react-redux';\r\nimport familyPlanService from '../../../../services/familyPlanService';\r\nimport debounce from 'lodash.debounce';\r\nimport InviteUserCard from './InviteUserCard';\r\nimport { sendRequest } from '../../../../reducers/invitationReducer';\r\nimport '../assets/InvitePeople.css';\r\n\r\nconst InvitePeople = ({ user }) => {\r\n  const { Option } = Select;\r\n  const dispatch = useDispatch();\r\n  const [queryResults, setQueryResults] = useState([]);\r\n  const [planForInvitation, setPlanForInvitation] = useState(null);\r\n\r\n  const invitations = useSelector(\r\n    ({ invitationReducer }) => invitationReducer.sent\r\n  );\r\n  const familyPlans = useSelector(({ familyPlanReducer }) => familyPlanReducer);\r\n\r\n  const onSearch = async (e) => {\r\n    e.preventDefault();\r\n    const value = e.target.value;\r\n    if (value !== '') {\r\n      const { data } = await familyPlanService.searchUser(value),\r\n        { foundUsers } = data;\r\n      if (foundUsers) {\r\n        const result = foundUsers.filter(\r\n          (foundUser) => foundUser.id !== user.id\r\n        );\r\n        setQueryResults(result);\r\n      }\r\n    }\r\n  };\r\n\r\n  const sendInvite = (recepientId) => {\r\n    if (!planForInvitation)\r\n      return message.error('You have not selected a plan');\r\n\r\n    const [id, planName] = planForInvitation.split(':');\r\n    const params = {\r\n      recepientId,\r\n      planId: id,\r\n      requester: user.id,\r\n      planName: planName,\r\n    };\r\n    dispatch(sendRequest(params));\r\n  };\r\n\r\n  const handlePlanSelection = (val) => {\r\n    setPlanForInvitation(val);\r\n  };\r\n\r\n  const debounceHandler = useMemo(() => debounce(onSearch, 300), []);\r\n\r\n  // Stop the invocation of the debounced function\r\n  // after unmounting\r\n  useEffect(() => {\r\n    return () => debounceHandler.cancel();\r\n  }, []); // eslint-disable-line\r\n\r\n  return (\r\n    <div>\r\n      <Input placeholder=\"Enter username\" onChange={debounceHandler} />\r\n      <Select\r\n        placeholder=\"Select a plan to which you want to invite the user\"\r\n        onChange={handlePlanSelection}\r\n      >\r\n        {familyPlans.map((plan) => {\r\n          return (\r\n            <Option key={plan.id} value={`${plan.id}:${plan.planName}`}>\r\n              {plan.planName}\r\n            </Option>\r\n          );\r\n        })}\r\n      </Select>\r\n      <div className=\"text-container\">\r\n        <span className=\"secondary-text\">\r\n          Users found: {queryResults.length}\r\n        </span>\r\n      </div>\r\n      <div className=\"user-list\">\r\n        {queryResults &&\r\n          queryResults.map((foundUser) => {\r\n            return (\r\n              <InviteUserCard\r\n                key={foundUser.id}\r\n                {...{ foundUser, invitations, sendInvite }}\r\n              />\r\n            );\r\n          })}\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default InvitePeople;\r\n","/app/src/components/pages/FamilyPlanHome/extra/InviteUserCard.js",[],"/app/src/reducers/invitationReducer.js",[],"/app/src/services/familyPlanRequestService.js",["182"],"import axios from 'axios';\r\nconst baseUrl = 'http://localhost:3001/api/family-plan-request';\r\n\r\nconst getSentRequests = async (userId) => {\r\n  const { data } = await axios.get(`${baseUrl}/sent-requests/${userId}`);\r\n  return data;\r\n};\r\n\r\nconst sendRequest = async (params) => {\r\n  const { data } = await axios.post(`${baseUrl}/send-request`, params);\r\n  return data;\r\n};\r\n\r\nconst getReceivedRequests = async (userId) => {\r\n  console.log(`${baseUrl}/avaiting-response/${userId}`);\r\n  const { data } = await axios.get(`${baseUrl}/avaiting-response/${userId}`);\r\n  return data;\r\n};\r\n\r\nconst answerRequest = async (answer, userId, requestId) => {\r\n  const params = { answer, userId };\r\n  const { data } = await axios.patch(\r\n    `${baseUrl}/request-response/${requestId}`,\r\n    params\r\n  );\r\n  return data;\r\n};\r\n\r\nexport default {\r\n  getSentRequests,\r\n  sendRequest,\r\n  getReceivedRequests,\r\n  answerRequest,\r\n};\r\n","/app/src/components/pages/FamilyPlanHome/extra/MyInvitations.js",[],{"ruleId":"183","replacedBy":"184"},{"ruleId":"185","replacedBy":"186"},{"ruleId":"187","severity":1,"message":"188","line":21,"column":6,"nodeType":"189","endLine":21,"endColumn":25,"suggestions":"190"},{"ruleId":"187","severity":1,"message":"191","line":54,"column":66,"nodeType":"189","endLine":54,"endColumn":68,"suggestions":"192"},{"ruleId":"193","severity":1,"message":"194","line":29,"column":1,"nodeType":"195","endLine":34,"endColumn":3},"no-native-reassign",["196"],"no-negated-in-lhs",["197"],"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'location' and 'params'. Either include them or remove the dependency array.","ArrayExpression",["198"],"React Hook useMemo has a missing dependency: 'onSearch'. Either include it or remove the dependency array.",["199"],"import/no-anonymous-default-export","Assign object to a variable before exporting as module default","ExportDefaultDeclaration","no-global-assign","no-unsafe-negation",{"desc":"200","fix":"201"},{"desc":"202","fix":"203"},"Update the dependencies array to be: [location, location.pathname, params]",{"range":"204","text":"205"},"Update the dependencies array t