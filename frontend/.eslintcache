t.value;\r\n    if (value !== '') {\r\n      const { data } = await familyPlanService.searchUser(value),\r\n        { foundUsers } = data;\r\n      if (foundUsers) {\r\n        const result = foundUsers.filter(\r\n          (foundUser) => foundUser.id !== user.id\r\n        );\r\n        setQueryResults(result);\r\n      }\r\n    }\r\n  };\r\n\r\n  const sendInvite = (recepientId) => {\r\n    if (!planForInvitation)\r\n      return message.error('You have not selected a plan');\r\n\r\n    const [id, planName] = planForInvitation.split(':');\r\n    const params = {\r\n      recepientId,\r\n      planId: id,\r\n      requester: user.id,\r\n      planName: planName,\r\n    };\r\n    dispatch(sendRequest(params));\r\n  };\r\n\r\n  const handlePlanSelection = (val) => {\r\n    setPlanForInvitation(val);\r\n  };\r\n\r\n  const debounceHandler = useMemo(() => debounce(onSearch, 300), []);\r\n\r\n  // Stop the invocation of the debounced function\r\n  // after unmounting\r\n  useEffect(() => {\r\n    return () => debounceHandler.cancel();\r\n  }, []); // eslint-disable-line\r\n\r\n  return (\r\n    <div>\r\n      <Input placeholder=\"Enter username\" onChange={debounceHandler} />\r\n      <Select\r\n        placeholder=\"Select a plan to which you want to invite the user\"\r\n        onChange={handlePlanSelection}\r\n      >\r\n        {familyPlans.map((plan) => {\r\n          return (\r\n            <Option key={plan.id} value={`${plan.id}:${plan.planName}`}>\r\n              {plan.planName}\r\n            </Option>\r\n          );\r\n        })}\r\n      </Select>\r\n      <div className=\"text-container\">\r\n        <span className=\"secondary-text\">\r\n          Users found: {queryResults.length}\r\n        </span>\r\n      </div>\r\n      <div className=\"user-list\">\r\n        {queryResults &&\r\n          queryResults.map((foundUser) => {\r\n            return (\r\n              <InviteUserCard\r\n                key={foundUser.id}\r\n                {...{ foundUser, invitations, sendInvite }}\r\n              />\r\n            );\r\n          })}\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default InvitePeople;\r\n","/app/src/components/pages/FamilyPlanHome/extra/InviteUserCard.js",[],"/app/src/reducers/invitationReducer.js",[],"/app/src/services/familyPlanRequestService.js",["180"],"import axios from 'axios';\r\nconst baseUrl = 'http://localhost:3001/api/family-plan-request';\r\n\r\nconst getSentRequests = async (userId) => {\r\n  const { data } = await axios.get(`${baseUrl}/sent-requests/${userId}`);\r\n  return data;\r\n};\r\n\r\nconst sendRequest = async (params) => {\r\n  const { data } = await axios.post(`${baseUrl}/send-request`, params);\r\n  return data;\r\n};\r\n\r\nconst getReceivedRequests = async (userId) => {\r\n  console.log(`${baseUrl}/avaiting-response/${userId}`);\r\n  const { data } = await axios.get(`${baseUrl}/avaiting-response/${userId}`);\r\n  return data;\r\n};\r\n\r\nconst answerRequest = async (answer, userId, requestId) => {\r\n  const params = { answer, userId };\r\n  const { data } = await axios.patch(\r\n    `${baseUrl}/request-response/${requestId}`,\r\n    params\r\n  );\r\n  return data;\r\n};\r\n\r\nexport default {\r\n  getSentRequests,\r\n  sendRequest,\r\n  getReceivedRequests,\r\n  answerRequest,\r\n};\r\n","/app/src/components/pages/FamilyPlanHome/extra/MyInvitations.js",[],{"ruleId":"181","replacedBy":"182"},{"ruleId":"183","replacedBy":"184"},{"ruleId":"185","severity":1,"message":"186","line":54,"column":66,"nodeType":"187","endLine":54,"endColumn":68,"suggestions":"188"},{"ruleId":"189","severity":1,"message":"190","line":29,"column":1,"nodeType":"191","endLine":34,"endColumn":3},"no-native-reassign",["192"],"no-negated-in-lhs",["193"],"react-hooks/exhaustive-deps","React Hook useMemo has a missing dependency: 'onSearch'. Either include it or remove the dependency array.","ArrayExpression",["194"],"import/no-anonymous-default-export","Assign object to a variable befor